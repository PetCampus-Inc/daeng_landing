{
  "plugins": ["prettier-plugin-tailwindcss"],
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "arrowParens": "always",
  "semi": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "endOfLine": "lf",
  "proseWrap": "always",
  "bracketSpacing": true,
  "quoteProps": "as-needed"
}